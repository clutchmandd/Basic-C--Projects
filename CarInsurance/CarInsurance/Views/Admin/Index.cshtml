@using CarInsurance.Models
@model List<Insuree>

<div class="row no-gutters m-3">
  <div class="card col-sm-12 col-md-10 col-lg-6 mx-auto shadow">
    <div class="card-header text-center">
      <h5 class="m-0">Issued Quotes</h5>
    </div>
    <div class="card-body">
      @foreach (var signup in Model)
      {
          <input type="hidden" name="Id" value="@signup.Id" />
          <div class="row mb-3">
            <div class="col-2">
              @signup.FirstName
            </div>
            <div class="col">
              @signup.LastName
            </div>
            <div class="col">
              @signup.EmailAddress
            </div>
            <div class="col-5">
              @signup.Quote
            </div>
          </div>
       }
    </div>
  </div>
</div>